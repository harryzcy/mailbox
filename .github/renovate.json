{
  "extends": [
    "config:recommended",
    ":disableDependencyDashboard",
    ":automergeMinor"
  ],
  "labels": ["dependencies"],
  "packageRules": [
    {
      "matchPackagePatterns": [
        "^github.com/aws/aws-sdk-go-v2$",
        "^github.com/aws/aws-sdk-go-v2/",
        "^github.com/aws/smithy-go$"
      ],
      "automerge": true,
      "groupName": "all aws-sdk-go-v2 dependencies",
      "groupSlug": "all-aws-sdk-go-v2"
    },
    {
      "matchDatasources": ["golang-version"],
      "enabled": false
    }
  ],
  "postUpdateOptions": ["gomodTidy"]
}
