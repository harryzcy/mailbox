{
  "extends": [
    "config:base",
    "docker:pinDigests",
    "helpers:pinGitHubActionDigests",
    ":pinDependencies",
    ":disableDependencyDashboard",
    ":automergeMinor"
  ],
  "labels": [
    "dependencies"
  ],
  "packageRules": [
    {
      "packagePatterns": [
        "^github.com/aws/aws-sdk-go-v2$",
        "^github.com/aws/aws-sdk-go-v2/"
      ],
      "automerge": true,
      "groupName": "all aws-sdk-go-v2 dependencies",
      "groupSlug": "all-aws-sdk-go-v2"
    },
    {
      "matchDatasources": [
        "golang-version"
      ],
      "enabled": false
    }
  ],
  "postUpdateOptions": [
    "gomodTidy"
  ]
}
